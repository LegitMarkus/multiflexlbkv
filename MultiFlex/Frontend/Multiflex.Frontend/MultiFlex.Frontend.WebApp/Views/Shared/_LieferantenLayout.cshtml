<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Multiflex.WebApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Multiflex.WebApp.styles.css" asp-append-version="true" />
    <script src="../contrast-bootstrap-pro/js/cdb.js"></script>
            <script src="../contrast-bootstrap-pro/js/bootstrap.min.js"></script>
            <script src="https://kit.fontawesome.com/9d1d9a82d2.js" crossorigin="anonymous"></script>
</head>
<body>
    <div class="w-100">
        <main role="main" class="pb-3 w-100">
            @RenderBody()
        </main>
    </div>

   @await RenderSectionAsync("Scripts", required: true)

   <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
   <script type="text/javascript" >
       const { ipcRenderer } = require("electron");

        ipcRenderer.on('getLieferant-reply', (arg) => {
            google.charts.load('current', {'packages':['table']});
            google.charts.setOnLoadCallback(drawTable);

            function drawTable() {
            var data = new google.visualization.DataTable();
            data.addColumn('string', 'Name');
            data.addColumn('number', 'Salary');
            data.addColumn('boolean', 'Full Time Employee');
            data.addRows([
                ['Mike',  {v: 10000, f: '$10,000'}, true],
                ['Jim',   {v:8000,   f: '$8,000'},  false],
                ['Alice', {v: 12500, f: '$12,500'}, true],
                ['Bob',   {v: 7000,  f: '$7,000'},  true]
            ]);

            var table = new google.visualization.Table(document.getElementById('getLieferant-reply'));

            table.draw(data, {showRowNumber: true, width: '100%', height: '100%'});

            /*for (var j = 0; j < arg.length; j++) {
                document.getElementById('createRow').innerHTML += '<div class="col">'+ +'</div> <div class="col">'+ +'</div> <div class="col">'+ +'</div> <div class="col">ändern</div>';
            }*/
        });
   </script>
</body>
</html>